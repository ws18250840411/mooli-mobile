{
  "name": "mooli-mobile",
  "version": "1.8.2",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/mooli-mobile.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "mooli-tools clean",
    "start": "mooli-docs start  --env development",
    "copy": "copyfiles -f components/icon/style/font/* out dist/font",
    "build-site": "mooli-docs build --env production",
    "build-esm": "mooli-tools build --buildDir=components --type=babel --format=esm --copyStyle=true",
    "build-cjs": "mooli-tools build --buildDir=components --type=babel --format=cjs --copyStyle=true",
    "build-style": "mooli-tools build --buildDir=components --type=babel --format=style",
    "build-umd": "mooli-tools build --buildDir=components --type=rollup --format=umd",
    "build": "npm run clean && npm run build-esm && npm run build-cjs && npm run build-umd && npm run build-style && npm run build-site"
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "sideEffects": [
    "*.css",
    "*.less",
    "dist/es/index.js",
    "src/index.ts",
    "dist/es/global/index.js",
    "src/global/index.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/ws18250840411/mooli-mobile.git"
  },
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "alloy",
      "alloy/react",
      "alloy/typescript"
    ],
    "rules": {
      "global-require": 0,
      "import/no-dynamic-require": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/member-ordering": 0,
      "@typescript-eslint/prefer-optional-chain": 0,
      "@typescript-eslint/prefer-for-of": 0,
      "@typescript-eslint/no-unused-expressions": 0,
      "react/jsx-no-useless-fragment": 0,
      "no-new-func": 0,
      "react/no-unstable-nested-components": 0,
      "react/jsx-no-constructed-context-values": 0,
      "react/no-find-dom-node": 0,
      "react/no-did-update-set-state": 0,
      "prefer-promise-reject-errors": 0,
      "no-promise-executor-return": 0,
      "no-async-promise-executor": 0,
      "no-return-assign": 0,
      "no-eq-null": 0,
      "no-param-reassign": 0,
      "max-params": 0,
      "complexity": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "proseWrap": "never",
    "trailingComma": "all"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "mooli-carousel": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.170",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "copyfiles": "^2.4.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.26.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-plugin-react": "^7.23.2",
    "mooli-docs": "^1.3.9",
    "mooli-tools": "^1.3.6",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  },
  "umdGlobalDependencies": {
    "react": "React",
    "react-dom": "ReactDom",
    "react-transition-group": "reactTransitionGroup",
    "classnames": "classnames",
    "lodash": "lodash",
    "prop-types": "PropTypes",
    "mooli-carousel": "MooliCarousel"
  }
}
